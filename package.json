{
  "name": "electron-fiddle",
  "productName": "Electron Fiddle",
  "version": "0.24.0",
  "description": "The easiest way to get started with Electron",
  "repository": "https://github.com/electron/fiddle",
  "main": "./dist/src/main/main",
  "scripts": {
    "contributors": "node ./tools/contributors.js",
    "less": "node ./tools/lessc.js",
    "lint:style": "stylelint  \"./src/less/*.less\" --fix",
    "lint:ts": "eslint \"./**/*.{ts,tsx}\" --fix",
    "lint:templates": "standard \"static/show-me/**/*.js\" --fix",
    "lint:links": "node ./tools/check-links.js",
    "lint": "npm-run-all \"lint:*\"",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "parcel:build": "node ./tools/parcel-build.js",
    "parcel:watch": "node ./tools/parcel-watch.js",
    "publish": "electron-forge publish",
    "start": "rimraf ./dist && electron-forge start --",
    "test": "jest --config=jest.json",
    "test:ci": "jest --config=jest.json --coverage --runInBand",
    "test:report": "jest --config=jest.json --json --bail=false --outputFile=report.json | true",
    "tsc": "tsc --noEmit -p .",
    "electron-releases": "node --unhandled-rejections=strict ./tools/fetch-releases.js",
    "postinstall": "husky install"
  },
  "keywords": [
    "Electron",
    "Fiddle",
    "Tutorial"
  ],
  "author": "Felix Rieseberg <felix@felixrieseberg.com>",
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/select": "^3.15.0",
    "@electron/get": "^3.1.0",
    "@octokit/rest": "^17.0.0",
    "@sentry/electron": "^2.3.0",
    "@types/is-running": "^2.1.0",
    "classnames": "^2.2.6",
    "commander": "^7.1.0",
    "decomment": "^0.9.3",
    "decompress": "^4.0.1",
    "electron-default-menu": "^1.0.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^9.1.0",
    "is-running": "^2.1.0",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "monaco-editor": "^0.21.3",
    "monaco-loader": "1.0.0",
    "namor": "^2.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-mosaic-component": "^4.1.1",
    "semver": "^7.3.4",
    "shell-path": "^2.1.0",
    "tmp": "0.2.1",
    "tslib": "^2.1.0",
    "update-electron-app": "^2.0.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@electron-forge/cli": "^6.0.0-beta.55",
    "@electron-forge/maker-deb": "^6.0.0-beta.55",
    "@electron-forge/maker-rpm": "^6.0.0-beta.55",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.55",
    "@electron-forge/maker-zip": "^6.0.0-beta.55",
    "@electron-forge/publisher-github": "^6.0.0-beta.55",
    "@octokit/action": "^6.0.5",
    "@types/classnames": "^2.2.11",
    "@types/decomment": "^0.9.0",
    "@types/decompress": "^4.2.3",
    "@types/enzyme": "^3.10.8",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/log-symbols": "^3.0.0",
    "@types/node": "^12.18.0",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.11",
    "@types/semver": "^7.3.4",
    "@types/tmp": "0.2.0",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^4.15.2",
    "cross-fetch": "^3.1.0",
    "electron": "22.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.29.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^5.1.1",
    "jest": "^29.7.0",
    "json-stable-stringify": "^1.0.1",
    "less": "^4.1.1",
    "lint-staged": "^15.2.4",
    "log-symbols": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "standard": "^17.1.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.2.2"
  },
  "lint-staged": {
    "./**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "./static/show-me/**/*.js": [
      "standard --fix"
    ],
    "./src/less/*.less": [
      "stylelint --fix"
    ]
  }
}
